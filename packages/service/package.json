{
  "name": "@notion-image-cdn/service",
  "version": "1.0.0",
  "private": true,
  "description": "Core proxy microservice for Notion Image CDN",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun dist/index.js",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "eslint src/",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "fastify": "^5.2.0",
    "fastify-plugin": "^5.0.0",
    "@fastify/cors": "^11.0.0",
    "@fastify/rate-limit": "^10.2.0",
    "@fastify/helmet": "^13.0.0",
    "sharp": "^0.33.0",
    "ioredis": "^5.4.0",
    "@aws-sdk/client-s3": "^3.740.0",
    "zod": "^3.24.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "@types/bun": "latest"
  }
}

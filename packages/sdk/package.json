{
  "name": "notion-image-cdn",
  "version": "1.0.0",
  "description": "Rewrite Notion's expiring S3 image URLs to permanent, optimized CDN URLs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "eslint src/",
    "clean": "rm -rf dist .turbo"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.0.0",
    "bun-types": "^1.3.9",
    "react": "^19.0.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "notion",
    "image",
    "cdn",
    "proxy",
    "optimization",
    "s3",
    "cache"
  ],
  "license": "MIT",
  "author": "Divyanshu Soni",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcs-soni/notion-image-cdn"
  }
}
